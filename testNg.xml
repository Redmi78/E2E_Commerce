<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cucumber Suite" parallel="methods" thread-count="5">
    <test name="Cucumber Tests">
        <classes>
            <class name="testRunner.TestNGCucumberRunner"/>
        </classes>
    </test>
